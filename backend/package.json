{
  "name": "instant-class-chat-backend",
  "version": "1.0.0",
  "description": "Backend for Instant Class Chat",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "node test-auth.js",
    "db:generate": "npx prisma generate",
    "db:setup": "node database/setup.js",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset && npm run db:seed",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev"
  },
  "keywords": [
    "chat",
    "real-time",
    "messaging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "nodemon": "^3.1.10",
    "prisma": "^6.19.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
